    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.0",
    "csv-loader": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^5.0.2",
